<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import com.jfoenix.validation.*?>
<?import de.jensd.fx.glyphs.fontawesome.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Label?>
<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="740.0"
            prefWidth="1000.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.vecsight.dragonite.proxy.gui.controller.DragoniteController" stylesheets="Theme.css">
    <children>
        <LineChart fx:id="lcSystemLoad" layoutX="350.0" layoutY="55.0" prefHeight="200.0" prefWidth="300.0" minHeight="140.0"
                   minWidth="300" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1">
            <xAxis>
                <CategoryAxis fx:id="lcSystemLoadxAxis" side="BOTTOM"/>
            </xAxis>
            <yAxis>
                <NumberAxis fx:id="lcSystemLoadyAxis" prefHeight="109.0" prefWidth="26.0" side="LEFT"/>
            </yAxis>
        </LineChart>
        <LineChart fx:id="lcMemory" layoutX="650.0" layoutY="55.0" prefHeight="200.0" prefWidth="300.0"
                   minHeight="140.0" minWidth="300" xmlns="http://javafx.com/javafx/9"
                   xmlns:fx="http://javafx.com/fxml/1">
            <xAxis>
                <CategoryAxis fx:id="lcMemoryxAxis" side="BOTTOM"/>
            </xAxis>
            <yAxis>
                <NumberAxis fx:id="lcMemoryyAxis" prefHeight="109.0" prefWidth="26.0" side="LEFT"/>
            </yAxis>
        </LineChart>

        <JFXRippler position="BACK" styleClass="icons-started" layoutX="380.0" layoutY="310.0">
            <StackPane style="-fx-padding: 10;">
                <FontAwesomeIconView fx:id="faiConfigSaved" glyphName="CLOSE" size="2em" />
            </StackPane>
        </JFXRippler>
        <Label fx:id="lbConfigSaved" text="Config Not Saved ..." styleClass="jfx-text-field" layoutX="435.0" layoutY="325.0" />

        <JFXRippler position="BACK" styleClass="icons-started" layoutX="380.0" layoutY="350.0">
            <StackPane style="-fx-padding: 10;">
                <FontAwesomeIconView fx:id="faiStarted" glyphName="STOP_CIRCLE" size="2em" />
            </StackPane>
        </JFXRippler>
        <Label fx:id="lbStarted" text="Proxy Stopped ..." styleClass="jfx-text-field" layoutX="435.0" layoutY="365.0" />

        <JFXProgressBar fx:id="pbGoogleStatus" layoutX="450.0" layoutY="600.0" prefWidth="100.0" />
        <JFXButton fx:id="btStart" layoutX="57.0" layoutY="580.0" mnemonicParsing="false"
                   onAction="#dragoniteProxyStart"
                   text="START" style="-jfx-button-type: RAISED" styleClass="button-raised"/>
        <JFXButton fx:id="btStop" layoutX="186.0" layoutY="580.0" mnemonicParsing="false" onAction="#dragoniteProxyStop"
                   text="STOP" style="-jfx-button-type: RAISED" styleClass="button-raised"/>
        <JFXButton fx:id="btSave" layoutX="313.0" layoutY="580.0" mnemonicParsing="false" onAction="#dragoniteProxySave"
                   text="SAVE" style="-jfx-button-type: RAISED" styleClass="button-raised"/>
        <GridPane layoutX="60.0" layoutY="55.0" prefHeight="316.0" prefWidth="321.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="240.0" minWidth="10.0" prefWidth="321.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="70.0" prefHeight="80.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="70.0" prefHeight="80.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="70.0" prefHeight="80.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="70.0" prefHeight="80.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="70.0" prefHeight="80.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="70.0" prefHeight="80.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="70.0" prefHeight="80.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <JFXTextField fx:id="tfServer" promptText="ServerAddress" labelFloat="true">
                    <validators>
                        <RequiredFieldValidator message="Input Required!">
                            <FontAwesomeIconView glyphName="WARNING" style="-fx-font-family: FontAwesome;"/>
                        </RequiredFieldValidator>
                    </validators>
                </JFXTextField>
                <JFXPasswordField fx:id="pfPassword" promptText="Password" labelFloat="true" GridPane.rowIndex="1">
                    <validators>
                        <RequiredFieldValidator message="Input Required!">
                            <FontAwesomeIconView glyphName="WARNING" style="-fx-font-family: FontAwesome;"/>
                        </RequiredFieldValidator>
                    </validators>
                </JFXPasswordField>
                <JFXTextField fx:id="tfServerPort" promptText="ServerPort" labelFloat="true" GridPane.rowIndex="2">
                    <validators>
                        <RequiredFieldValidator message="Input Required!">
                            <FontAwesomeIconView glyphName="WARNING" style="-fx-font-family: FontAwesome;"/>
                        </RequiredFieldValidator>
                    </validators>
                </JFXTextField>
                <JFXTextField fx:id="tfLocalPort" promptText="LocalPort" labelFloat="true" GridPane.rowIndex="3">
                    <validators>
                        <RequiredFieldValidator message="Input Required!">
                            <FontAwesomeIconView glyphName="WARNING" style="-fx-font-family: FontAwesome;"/>
                        </RequiredFieldValidator>
                    </validators>
                </JFXTextField>
                <JFXTextField fx:id="tfDownloadMbps" promptText="DownloadMbps" labelFloat="true" GridPane.rowIndex="4">
                    <validators>
                        <RequiredFieldValidator message="Input Required!">
                            <FontAwesomeIconView glyphName="WARNING" style="-fx-font-family: FontAwesome;"/>
                        </RequiredFieldValidator>
                    </validators>
                </JFXTextField>
                <JFXTextField fx:id="tfUploadMbps" promptText="UploadMbps" labelFloat="true" GridPane.rowIndex="5">
                    <validators>
                        <RequiredFieldValidator message="Input Required!">
                            <FontAwesomeIconView glyphName="WARNING" style="-fx-font-family: FontAwesome;"/>
                        </RequiredFieldValidator>
                    </validators>
                </JFXTextField>
                <JFXTextField fx:id="tfMTU" promptText="MTU" labelFloat="true" GridPane.rowIndex="6"/>

            </children>
        </GridPane>
    </children>
    <padding>
        <Insets bottom="2.0" right="2.0" top="2.0"/>
    </padding>
</AnchorPane>
